{
  "name": "ledger-charts",
  "version": "0.1.0",
  "description": "Simple way to draw basic charts from your ledger accounting file",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "tscw": "tsc --p . -w",
    "tsc": "tsc --p .",
    "startangular": "tsc && concurrently \"npm run-script tscw\" \"npm run lite\" ",
    "lite": "lite-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Benlenem/ledger-charts.git"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "FileSaver": "^0.10.0",
    "concurrently": "^2.2.0",
    "electron": "^1.3.4",
    "lite-server": "^2.2.2",
    "typescript": "^2.0.2",
    "typings": "^1.3.2"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "@types/moment": "2.13.0",
    "@types/xml2js": "0.0.28",
    "angular2-in-memory-web-api": "0.0.20",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.1",
    "pegjs": "^0.10.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "xml2js": "^0.4.17",
    "zone.js": "^0.6.23"
  }
}
