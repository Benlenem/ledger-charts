{
  "name": "ledger-charts",
  "version": "0.1.0",
  "description": "Simple way to draw basic charts from your ledger accounting file",
  "main": "main.js",
  "scripts": {
    "cssw": "nodemon -e scss -x \"npm run css\"",
    "css": "node-sass --include-path css css/app.scss css/app.css",
    "start": "electron .",
    "tscw": "tsc --p . -w",
    "tsc": "tsc --p .",
    "lite": "lite-server -w \"src/**/*.css # #reloadcss \"",
    "dev": "concurrently --kill-others \"npm run tscw\" \"npm run cssw\" \"npm run lite\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Benlenem/ledger-charts.git"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "concurrently": "^3.2.0",
    "electron": "^1.3.4",
    "lite-server": "^2.2.2",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "sass": "^0.5.0",
    "typings": "^1.3.2"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "@types/filesaver": "0.0.30",
    "@types/moment": "2.13.0",
    "@types/xml2js": "0.0.28",
    "angular2-in-memory-web-api": "0.0.20",
    "chart.js": "2.3.0",
    "core-js": "2.4.1",
    "file-saver": "1.3.3",
    "FileSaver": "0.10.0",
    "moment": "2.15.2",
    "pegjs": "0.10.0",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "typescript": "2.0.2",
    "xml2js": "0.4.17",
    "zone.js": "0.6.23"
  }
}
